<div class="container-form">
  <%= simple_form_for [@profile] do |f| %>
  <div class="form-login">
    <%= f.input :description_user %>
    <%= f.input :address, input_html: { id: 'gmaps-input-address' }, :placeholder => 'Start typing a place name...' %>
    <%= f.input :phone %>
    <%= f.input :photoprofile %>
    <%= f.input :photoprofile_cache, as: :hidden %>
    <div class="text-center"> <%= f.button :submit, class: 'btn btn-success' %> </div>
  </div>
  <% end %>
</div>

